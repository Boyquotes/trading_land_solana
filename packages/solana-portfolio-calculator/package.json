{
  "name": "@trading-land/solana-portfolio-calculator",
  "version": "1.0.0",
  "description": "A TypeScript library for calculating Solana wallet portfolios with token metadata and pricing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsx src/examples/example.ts",
    "server": "tsx src/examples/server.ts",
    "test-all": "npm run demo && echo '\n' && npm run basic"
  },
  "keywords": [
    "solana",
    "portfolio",
    "wallet",
    "tokens",
    "nft",
    "cryptocurrency",
    "blockchain",
    "typescript",
    "defi"
  ],
  "author": "Trading Land <contact@trading-land.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trading-land/solana-portfolio-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/trading-land/solana-portfolio-calculator/issues"
  },
  "homepage": "https://github.com/trading-land/solana-portfolio-calculator#readme",
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@solana/spl-token": "^0.4.1",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": ">=1.90.0",
    "axios": "^1.6.2",
    "dotenv": "^17.2.2",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.90.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
